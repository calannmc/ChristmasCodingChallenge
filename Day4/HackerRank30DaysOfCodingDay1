import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        // Declare variables
        int i = 4; // Type int
        double d = 4.0; // Type double
        String s = "HackerRank "; // Type string

        // Create a Scanner object to read input from STDIN
        Scanner scanner = new Scanner(System.in);

        // Read input
        int iInput = scanner.nextInt();
        double dInput = scanner.nextDouble();
        scanner.nextLine(); // Consume the newline character left after reading double
        String sInput = scanner.nextLine();

        // Perform operations
        int sumInt = i + iInput;
        double sumDouble = d + dInput;
        String concatString = s + sInput;

        // Print output
        System.out.println(sumInt);
        System.out.println(String.format("%.1f", sumDouble));
        System.out.println(concatString);

        // Close the scanner
        scanner.close();
    }
}
